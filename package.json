  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@types/jest": "^24.0.11",
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-state": "^0.4.2",
    "graphql": "^14.2.1",
