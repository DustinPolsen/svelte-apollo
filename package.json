    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-typescript": "^1.0.1",
    "sirv-cli": "^0.4.0",
    "svelte": "^3.0.0",
    "ts-jest": "^24.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
