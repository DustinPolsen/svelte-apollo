  "main": "dist/svelte-apollo.cjs.js",
  "module": "dist/svelte-apollo.es.js",
  "types": "dist/svelte-apollo.d.ts",
  "keywords": [
    "svelte",
    "apollo",
    "graphql"
  ],
  "scripts": {
